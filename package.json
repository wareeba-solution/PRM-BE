    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.9",
    "esl": "^11.2.1",
    "express": "^4.18.2",
    "firebase-admin": "^13.2.0",
    "handlebars": "^4.7.8",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "liquidjs": "^10.9.4",
    "mjml": "^4.15.3",
    "module-alias": "^2.2.3",
    "nestjs-typeorm-paginate": "^4.1.0",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.14.1",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.5.1",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1"
